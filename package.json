{
  "name": "cblhub-web",
  "version": "0.1.0",
  "description": "CBLHub webapp",
  "main": "./app",
  "scripts": {
    "start": "node ./",
    "test": "istanbul cover -x *.spec.js _mocha -- --require babel-register --require ignore-styles app/**/*.spec.js",
    "dev": "concurrently --kill-others 'nodemon ./' 'node webpack/webpack-dev-server'",
    "coverage": "npm test --coverage && open ./coverage/lcov-report/index.html",
    "lint": "eslint app/** -c ./.eslintrc",
    "build": "rimraf dist/ && webpack --config webpack/webpack.config",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diegohaz/cblhub-web.git"
  },
  "author": "Diego Haz <hazdiego@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/diegohaz/cblhub-web/issues"
  },
  "homepage": "https://github.com/diegohaz/cblhub-web#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "concurrently": "^2.1.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.11.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-config-standard-react": "^2.4.0",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-standard": "^1.3.2",
    "expect": "^1.20.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "ignore-styles": "^4.0.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.5.3",
    "nock": "^8.0.0",
    "nock-vcr-recorder": "^0.1.3",
    "nock-vcr-recorder-mocha": "^0.3.2",
    "node-sass": "^3.7.0",
    "nodemon": "^1.9.2",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^3.0.0-beta.1",
    "redux-mock-store": "^1.1.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "axios": "^0.12.0",
    "babel-polyfill": "^6.9.1",
    "babel-register": "^6.9.0",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "css-loading": "^1.4.0",
    "denormalizr": "^0.2.0",
    "express": "^4.13.4",
    "express-https-redirect": "^1.0.0",
    "foundation-sites": "^6.2.3",
    "lodash": "^4.13.1",
    "normalizr": "^2.1.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-helmet": "^3.1.0",
