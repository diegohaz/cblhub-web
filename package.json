{
  "name": "cblhub-web",
  "version": "0.1.0",
  "description": "CBLHub webapp",
  "main": "./app",
  "scripts": {
    "start": "node ./",
    "test": "mocha -r babel-register -r ignore-styles 'app/**/*.spec.js'",
    "dev": "concurrently --kill-others 'nodemon ./' 'node webpack/webpack-dev-server'",
    "coverage": "istanbul cover -x *.spec.js _mocha -- -r babel-register -r ignore-styles 'app/**/*.spec.js'",
    "coverage-open": "npm run coverage && open ./coverage/lcov-report/index.html",
    "lint": "eslint --fix app/** -c ./.eslintrc",
    "build": "rimraf dist/ && webpack --config webpack/webpack.config",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diegohaz/cblhub-web.git"
  },
  "author": "Diego Haz <hazdiego@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/diegohaz/cblhub-web/issues"
  },
  "homepage": "https://github.com/diegohaz/cblhub-web#readme",
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "concurrently": "^2.1.0",
    "enzyme": "^2.3.0",
    "eslint": "^3.3.1",
    "eslint-config-standard": "^5.3.5",
    "eslint-config-standard-react": "^3.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.1.2",
    "eslint-plugin-standard": "^2.0.0",
    "expect": "^1.20.1",
    "file-loader": "^0.9.0",
    "ignore-styles": "^4.0.0",
    "istanbul": "^1.0.0-alpha.2",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.2",
    "nock": "^8.0.0",
    "nodemon": "^1.9.2",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^3.0.0-beta.1",
    "redux-mock-store": "^1.1.0",
    "rimraf": "^2.5.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "axios": "^0.13.1",
    "babel-polyfill": "^6.9.1",
    "babel-register": "^6.9.0",
    "bluebird": "^3.4.1",
    "compression": "^1.6.2",
    "css-loading": "^1.4.0",
    "denormalizr": "^0.3.2",
    "express": "^4.13.4",
    "express-https-redirect": "^1.0.0",
    "lodash": "^4.13.1",
    "moment": "^2.13.0",
    "normalizr": "^2.1.0",
    "radium": "^0.18.1",
    "react": "^15.1.0",
    "react-aria-menubutton": "^4.1.2",
    "react-cookie": "^0.4.7",
    "react-dom": "^15.1.0",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
